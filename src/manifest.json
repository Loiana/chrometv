{
  "manifest_version": 2,

  "name": "chrometv ",
  "description": "Chrome TV",
  "version": "1.1",

  "background": {
    "scripts": ["lib/jquery-2.0.3.min.js", "js/utils.js", "js/chrometv.js"]
  },

  "permissions": [
    "tabs", "webNavigation", "http://www.google.com.br/",
    "https://spreadsheets.google.com/", "https://accounts.google.com/",
    "https://docs.google.com/", "http://www.dextra.com.br/",
    "http://dextra.com.br/", "https://appengine.google.com/"
  ],

  "icons": {
    "128": "images/icon_128.png"
  }
}
